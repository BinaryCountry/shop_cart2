<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sise.shop_cart2.mapper.ItemMapper">
    <select id="showAllitem" resultType="com.sise.shop_cart2.pojo.Item">
        select * from item
    </select>

    <select id="findItemByID" parameterType="int" resultType="com.sise.shop_cart2.pojo.Item">
        select * from item  where id=#{id};
    </select>


</mapper>